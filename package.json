    "chart.js": "^2.8.0",
    "classnames": "^2.2.3",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "jquery": "^3.4.1",
    "materialize-css": "^1.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.6",
    "react-chartjs": "^1.2.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "redis": "^2.4.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
