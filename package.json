{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf src/public",
    "start": "node src/server.js",
    "dev": "node webpack/webpack.devserver.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --watch",
    "build:webpack": "NODE_ENV=production webpack --progress --colors --config webpack/webpack.config.js",
    "build": "npm run clean && npm run build:webpack",
    "deploy:heroku": "git push heroku master",
    "deploy:github": "git push origin master",
    "deploy": "npm run build && npm run deploy:github && npm run deploy:heroku"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.2.1",
